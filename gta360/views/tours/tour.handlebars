           style="padding-bottom: 90px; background-size: cover !important;">
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <div class="row">
            <div class="col-sm-2">
              <figure>
                <img src="{{tour.creator.picture}}" class="img-responsive" />
              </figure>
            </div>
            <div class="col-sm-2">
              <h5 class="tour-title agent-name">YOUR AGENT</h5>
              <p>{{tour.creator.name.full}}</p>
              {{#if tour.creator.title}}
                <span style="display: block;"><strong>Title: </strong>{{tour.creator.title}}</span>
              {{/if}}
              {{#if tour.creator.brokerage}}
                <span style="display: block;"><strong>Brokerage: </strong>{{tour.creator.brokerage}}</span>
              {{/if}}
            </div>
            <div class="col-sm-5">
              <h5 class="tour-title" style="color: #FAB702;">ABOUT ME</h5>
              <div class="widget widget-address">
                {{#if tour.creator.description}}
                  <p style="margin-top: 20px;">{{tour.creator.description}}</p>
                {{/if}}
              </div>
            </div>
            <div class="col-sm-3">
              <h5 class="tour-title" style="color: #FAB702; margin-bottom: 0;">CONTACT</h5>
              <div class="widget text-center">
                <div class="row">
                  <div class="col-sm-12">
                    <div class="widget widget-address text-left" >
                      <address>
                        {{#if tour.creator.contact.phone}}
                          <span><i class="fa fa-phone fa-lg" aria-hidden="true"></i>
                  <a href="tel:{{tour.creator.contact.phone_link_format}}">{{tour.creator.contact.phone}}</a></span>
                        {{/if}}
                        {{#if tour.creator.address}}
                          <span><i class="fa fa-map-marker fa-lg" aria-hidden="true"></i>
                            {{tour.creator.address}}</span>
                        {{/if}}

                        {{#if tour.creator.email}}
                          <span>
                  <i class="fa fa-envelope fa-lg" aria-hidden="true" style="font-size: 15px;"></i>
                  <a href="mailto:{{tour.creator.email}}">{{tour.creator.email}}</a>
                </span>
                        {{/if}}
                        {{#if tour.creator.website}}
                          <span>
                  <i class="fa fa-desktop fa-lg" aria-hidden="true"></i>
                  <a href="{{tour.creator.website}}">{{tour.creator.website}}</a>
                </span>
                        {{/if}}
                      </address>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  {{/if}}
  {{/unless}}
  <!-- Modal -->
  <div class="modal fade" id="contactModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <form name="contactForm" id='contact_form' method="post" action='{{url}}/contact-form'>
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h3 class="modal-title" id="myModalLabel">Send Your Agent Message</h3>
        </div>
        <div class="modal-body">
            <div class="row">
              <div class="col-md-4">
                <div id='name_error' class='error'>Please enter your name.</div>
                <div>
                  <input type='text' name='name' id='name' class="form-control" placeholder="Your Name">
                </div>

                <div id='email_error' class='error'>Please enter your valid E-mail ID.</div>
                <div>
                  <input type='text' name='email' id='email' class="form-control" placeholder="Your Email">
                </div>

                <div id='phone_error' class='error'>Please enter your phone number.</div>
                <div>
                  <input type='text' name='phone' id='phone' class="form-control" placeholder="Your Phone">
                </div>
              </div>
              <div class="col-md-8">
                <div id='message_error' class='error'>Please enter your message.</div>
                <div>
                  <textarea name='message' id='message' class="form-control" placeholder="Your Message"></textarea>
                </div>
              </div>

              <div class="col-md-12">
                <p id='submit' class="text-right">
                  <input type='submit' id='send_message' value='Send Message' class="btn btn-line">
                </p>
                <div id='mail_success' class='success'>Your message has been sent successfully.</div>
                <div id='mail_fail' class='error'>Sorry, error occured this time sending your message.</div>
              </div>
            </div>
        </div>
        </form>
      </div>
    </div>
  </div>
</div>

