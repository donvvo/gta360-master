                    {{#if tour.features.highlight3}}<li>{{tour.features.highlight3}}</li>{{/if}}
                    {{#if tour.features.highlight4}}<li>{{tour.features.highlight4}}</li>{{/if}}
                    {{#if tour.features.highlight5}}<li>{{tour.features.highlight5}}</li>{{/if}}
                    {{#if tour.features.highlight6}}<li>{{tour.features.highlight6}}</li>{{/if}}
                    {{#if tour.features.highlight7}}<li>{{tour.features.highlight7}}</li>{{/if}}
                    {{#if tour.features.highlight8}}<li>{{tour.features.highlight8}}</li>{{/if}}
                    {{#if tour.features.highlight9}}<li>{{tour.features.highlight9}}</li>{{/if}}
                    {{#if tour.features.highlight10}}<li>{{tour.features.highlight10}}</li>{{/if}}
