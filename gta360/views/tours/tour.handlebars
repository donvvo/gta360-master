  <!-- Gallery begin -->
  <section id="section-portfolio" class="no-bottom" style="margin-top: 0;
padding-top: 60px;">
    <div class="container">
      <div class="row">
        <div class="col-md-6 col-md-offset-3 text-center wow fadeInUp">
          <h1>Gallery</h1>
          <div class="separator"><span><i class="fa fa-circle"></i></span></div>
          <div class="spacer-single"></div>
        </div>
      </div>
    </div>

    <div id="gallery" class="gallery full-gallery de-gallery pf_full_width gallery-tour">
      {{#each tour.gallery}}
      <!-- gallery item -->
      <div class="item" data-index="{{@index}}">
        <div class="picframe">
          <a class="image-popup-gallery" href="{{photo}}">
            <span class="overlay">
              <span class="pf_text">
                <span class="project-name">{{name}}</span>
              </span>
            </span>
          </a>
          <img src="{{photo}}" alt="" />
        </div>
      </div>
      <!-- close gallery item -->
      {{/each}}
    </div>

    <div class="text-center">
      <button id="LoadMoreButton" class="btn btn-line btn-edit">
        Load More</button>
    </div>
  </section>
  <!-- Gallery close -->

  <!-- Sub slider begin -->
